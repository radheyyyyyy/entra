generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
// annoucments
// link,src,data(title),date

model annoucments{
  id Int @id @default(autoincrement())
  link String 
  source String
  title String
  date DateTime
}