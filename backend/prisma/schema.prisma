generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model announcments {
  id       String   @id @unique
  link     String
  source   String
  title    String
  date     DateTime
  type     String
  location String

  @@unique([link, source, title])
}
